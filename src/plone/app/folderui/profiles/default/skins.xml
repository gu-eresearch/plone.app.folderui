<?xml version="1.0"?>
<object name="portal_skins"
    meta_type="Plone Skins Tool"
    allow_any="False"
    cookie_persistence="False"
    request_varname="plone_skin">
    <object name="folderui"
        meta_type="Filesystem Directory View"
        directory="plone.app.folderui:skins/folderui"/>
    <skin-path name="*">
        <layer insert-after="custom" name="folderui"/>
    </skin-path>
</object>


